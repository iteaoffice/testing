{
  "name": "iteaoffice/testing",
  "type": "library",
  "license": "proprietary",
  "description": "General code for testing",
  "keywords": [
    "test"
  ],
  "homepage": "https://itea3.org",
  "support": {
    "email": "ticket@itea3.org"
  },
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "phpunit/phpunit": "@stable",
    "pdepend/pdepend": "@stable",
    "friendsofphp/php-cs-fixer": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "phploc/phploc": "@stable",
    "doctrine/data-fixtures": "@stable",
    "jakub-onderka/php-parallel-lint": "@stable",
    "jakub-onderka/php-console-highlighter": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Testing\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
